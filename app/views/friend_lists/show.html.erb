<p style="color: green"></p>





<div class="jumbotron">
  <h1 class="display-4"><%= @friend_list.first_name%> <%= @friend_list.last_name %></h1>
  <p class="lead">
      <%= @friend_list.email %> |
      <%= @friend_list.phone %> |
      <%= @friend_list.insta %> |

  </p>
  <hr class="my-4">
  <p></p>
  <a 
  
   <% if user_signed_in? %>
  <%= link_to "Edit this friend list", edit_friend_list_path(@friend_list), class:"btn btn-info btn-sm " %> <% end %> |
  <%= link_to "Back to friend lists", friend_lists_path, class:"btn btn-warning btn-sm" %>

  <br>
  <br>
  <% if user_signed_in? %>
  <%= button_to "Destroy this friend list",  @friend_list, method: :delete, class:"btn btn-danger" %>
  <% end %>
</a>
</div>
